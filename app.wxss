/**app.wxss**/
page{
  height:100%;
  overflow:hidden;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  line-height: 1.6;
} 
/** 修改默认的navigator点击态 **/
.navigator-hover{
  background-color:rgba(37,148,255,0.5);
}
/** 按钮样式 **/
.btn{
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    color: #FFFFFF;
    line-height: 2.55555556;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}
/** 隐藏滚动条 **/
::webkit-scrollbar{
  width:0;
  height:0;
  color:transparent;
}